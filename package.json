{
  "name": "@akijoey/stylelint-config",
  "version": "1.0.0",
  "description": "Personal stylelint config.",
  "keywords": [
    "stylelint"
  ],
  "repository": "github:akijoey/stylelint-config",
  "homepage": "https://github.com/akijoey/stylelint-config#readme",
  "bugs": "https://github.com/akijoey/stylelint-config/issues",
  "author": "AkiJoey <akijoey@akijoey.com> (https://akijoey.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "index.js",
    ".stylelintrc.js"
  ],
  "scripts": {
    "lint": "dev-scripts lint",
    "test": "dev-scripts test",
    "format": "dev-scripts format",
    "prepare": "dev-scripts hook"
  },
  "dependencies": {
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "peerDependencies": {
    "stylelint": "^13.8.0"
  },
  "devDependencies": {
    "@akijoey/dev-scripts": "latest"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@akijoey"
  },
  "prettier": "@akijoey/prettier-config",
  "commitlint": {
    "extends": "@akijoey/commitlint-config"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.{js,json,yml}": "npm run format"
  }
}
